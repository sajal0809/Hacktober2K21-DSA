#include <iostream>
#include <cmath>
#define ll long long
using namespace std;

int main()
{
    ll t;
    cin >> t;
    while (t--)
    {
        ll n;
        cin >> n;
        ll a=1;
        while(2*a<=n){
            a=a*2;
        }
        if(n==1||n==2){
            cout<<"1"<<endl;
        }
        else{
            if((a/2)>n-a+1){
                cout<<a/2<<endl;
            }
            else{
                cout<<n-a+1<<endl;
            }
        }
    }
    return 0;
}
